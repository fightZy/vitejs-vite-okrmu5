{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2017",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "removeComments": false,
    "allowSyntheticDefaultImports": true
  }
}
